   "source": [
    "path_dir = r\"D:/.../PR Files/\"\n",
    "#,
    "import pandas as pd\n",
    "import numpy as np\n",
    "import glob, os\n",
    "import re\n",
    "\n",
    "lst_df = []\n",
    "files_xlsx = glob.glob(path_dir + \"*.xlsx\")\n",
    "for f_xlsx in files_xlsx:\n",
    "    f_xlsx = str(f_xlsx)\n",
    "    df = pd.read_excel(f_xlsx, engine='openpyxl')\n",
    "    f_xlsx_1 = f_xlsx.rpartition(\"PR Files\")[-1].split('_')[0]\n",
    "    f_xlsx_2 = \" \".join(re.findall(\"[a-zA-Z]+\", f_xlsx_1))\n",
    "    df.loc[:, 'Purchasing_Division'] = f_xlsx_2\n",
    "    lst_df.append(df)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "metadata": {},
   "outputs": [],
   "source": [
    "purchase_master_0 = pd.concat(lst_df)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "metadata": {},
   "outputs": [],
   "source": [
    "path_cost = r\"D:/.../Map Files/Cost Center Mapping.xlsx\"\n",
    "cost = pd.read_excel(path_cost, engine='openpyxl')\n",
    "cost.rename(columns={'Cost Ctr': 'Cost Center'}, inplace=True)\n",
    "purchase_master_1 = pd.merge(purchase_master_0, cost, on='Cost Center', how='left')\n",
    "\n",
    "path_rates = r\"D:\\...\\Map Files\\Historical Rates.xlsx\"\n",
    "rates = pd.read_excel(path_rates, engine='openpyxl')\n",
    "rates['Date'] = pd.to_datetime(rates['Date'])\n",
    "rates['Date'] = rates['Date'].dt.strftime('%Y.%m.%d')\n",
    "\n",
    "rates.rename(columns={'Date': 'Document Date'}, inplace=True)\n",
    "purchase_master_2 = pd.merge(purchase_master_1, rates, on='Document Date', how='left')\n",
    "purchase_master_2['Net_Order_Value_USD'] = purchase_master_2['Net Order Value'] / purchase_master_2['THB Rate']\n",
    "purchase_master_2['Net_Order_Value_USD'] = purchase_master_2['Net_Order_Value_USD'].map('${:,.2f}'.format)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "metadata": {},
   "outputs": [],
   "source": [
    "path_material = r\"D:\\...\\Map Files\\Material Mapping.xlsx\"\n",
    "material = pd.read_excel(path_material, engine='openpyxl')\n",
    "purchase_master_3 = pd.merge(purchase_master_2, material, on='Material Code', how='left')\n",
    "\n",
    "path_plant = r\"D:\\PowerBI\\Map Files\\Plant Mapping.xlsx\"\n",
    "plant = pd.read_excel(path_plant, engine='openpyxl')\n",
    "purchase_master_4 = pd.merge(purchase_master_3, plant, on='Plant', how='left')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 37,
   "metadata": {},
   "outputs": [],
   "source": [
    "path_vendor = r\"D:/.../Map Files/Vendor Mapping.xlsx\"\n",
    "vendor = pd.read_excel(path_vendor, engine='openpyxl')\n",
    "vendor.rename(columns={'Vendor/supplying': 'Vendor/supplying plant'}, inplace=True)\n",
    "purchase_master_5 = pd.merge(purchase_master_4, vendor, on='Vendor/supplying plant', how='left')"
   ]
  },
